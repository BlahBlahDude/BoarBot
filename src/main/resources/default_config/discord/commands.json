  "manage": {
    "name": "boar-manage",
    "default_member_permissions": 32,
    "description": "Commands to manage BoarBot",
    "subcommands": {
      "setup": {
        "name": "setup",
        "description": "Set BoarBot up to your liking",
        "location": "dev.boarbot.commands.boarmanage.SetupSubcommand"
      }
    }
  },
  "dev": {
    "name": "boar-dev",
    "default_member_permissions": 0,
    "description": "Commands exclusive to developers of BoarBot",
    "subcommands": {
      "give": {
        "name": "give",
        "description": "Give a user a boar/badge",
        "location": "dev.boarbot.commands.boardev.GiveSubcommand",
        "options": [
          {
            "name": "user",
            "type": 6,
            "description": "User you'd like to give the item to",
            "required": true
          },
          {
            "name": "id",
            "type": 3,
            "description": "ID of item to give",
            "required": true,
            "autocomplete": true
          }
        ]
      },
      "ban": {
        "name": "ban",
        "description": "Ban a user from certain BoarBot features",
        "location": "dev.boarbot.commands.boardev.BanSubcommand",
        "options": [
          {
            "name": "user",
            "type": 6,
            "description": "User to ban",
            "required": true
          },
          {
            "name": "duration",
            "type": 4,
            "description": "Duration of ban in hours"
          }
        ]
      },
      "maintenance": {
        "name": "maintenance",
        "description": "Put BoarBot into maintenance mode",
        "location": "dev.boarbot.commands.boardev.MaintenanceSubcommand"
      }
    }
