        "name": "guess",
        "description": "Guess a value for hunts",
        "location": "dev.boarbot.commands.boar.GuessSubcommand",
        "options": [
          {
            "name": "value",
            "type": 3,
            "description": "Value to guess",
            "required": true
          }
        ]
