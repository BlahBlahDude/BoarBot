{
  "miracleAmount": {
    "id": "MIRACLE_AMOUNT",
    "title": "Miracle Charms Input",
    "components": [
      {
        "type": 4,
        "custom_id": "AMOUNT",
        "style": 1,
        "label": "Number of Miracle Charms to use",
        "max_length": 9,
        "placeholder": "Ex: 117"
      }
    ]
  },
  "pageInput": {
    "id": "PAGE_INPUT",
    "title": "Page Input",
    "components": [
      {
        "type": 4,
        "custom_id": "PAGE",
        "style": 1,
        "label": "Page number",
        "max_length": 9,
        "placeholder": "Ex: 3"
      }
    ]
  },
  "findBoar": {
    "id": "FIND_BOAR",
    "title": "Find Item",
    "components": [
      {
        "type": 4,
        "custom_id": "SEARCH",
        "style": 1,
        "label": "Search term",
        "max_length": 64,
        "placeholder": "Ex: Explosionless Boar"
      }
    ]
  },
  "cloneAmount": {
    "id": "CLONE_AMOUNT",
    "title": "Cloning Serum Input",
    "components": [
      {
        "type": 4,
        "custom_id": "AMOUNT",
        "style": 1,
        "label": "Number of Cloning Serums to use",
        "max_length": 9,
        "placeholder": "Ex: 117"
      }
    ]
  },
  "pageBoardInput": {
    "id": "PAGE_BOARD_INPUT",
    "title": "Page/User Input",
    "components": [
      {
        "type": 4,
        "custom_id": "PAGE",
        "style": 1,
        "label": "Page number",
        "max_length": 9,
        "required": false,
        "placeholder": "Ex: 3"
      },
      {
        "type": 4,
        "custom_id": "USERNAME",
        "style": 1,
        "label": "Username",
        "max_length": 32,
        "required": false,
        "placeholder": "Ex: weslay."
      }
    ]
  },
  "reportInput": {
    "id": "REPORT_INPUT",
    "title": "Report a Bug/Exploit",
    "components": [
      {
        "type": 4,
        "custom_id": "TITLE",
        "style": 1,
        "label": "What are you reporting?",
        "max_length": 100,
        "required": true
      },
      {
        "type": 4,
        "custom_id": "DETAILS",
        "style": 2,
        "label": "Provide some details",
        "max_length": 1800,
        "required": true
      },
      {
        "type": 4,
        "custom_id": "LINK",
        "style": 1,
        "label": "Relevant media (include https://)",
        "max_length": 100,
        "required": false,
        "placeholder": "Ex: https://i.imgur.com/jvj0nsT.gif"
      }
    ]
  },
  "wipeInput": {
    "id": "WIPE_INPUT",
    "title": "Data Removal Request",
    "components": [
      {
        "type": 4,
        "custom_id": "CHECK_1",
        "style": 1,
        "label": "The word \"DELETE\"",
        "max_length": 6,
        "required": true,
        "placeholder": "Doing this will delete ALL of your data!"
      },
      {
        "type": 4,
        "custom_id": "CHECK_2",
        "style": 1,
        "label": "Your username (Not display name)",
        "max_length": 32,
        "required": true,
        "placeholder": "This action is instantaneous and irreversible!"
      }
    ]
  },
  "buyInput": {
    "id": "BUY_INPUT",
    "title": "Buy an Item",
    "components": [
      {
        "type": 4,
        "custom_id": "AMOUNT",
        "style": 1,
        "label": "Number to buy",
        "max_length": 9,
        "placeholder": "Ex: 3"
      }
    ]
  },
  "sellInput": {
    "id": "SELL_INPUT",
    "title": "Sell an Item",
    "components": [
      {
        "type": 4,
        "custom_id": "AMOUNT",
        "style": 1,
        "label": "Number to sell",
        "max_length": 9,
        "placeholder": "Ex: 3"
      }
    ]
  }
}